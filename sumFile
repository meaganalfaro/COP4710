
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
    FILE *fp;
    fp = fopen("numbers.txt", "r");
    if(fp == NULL){
        printf("Unable to open file\n");
        exit(1);
    }
    
    int num , sum = 0;
    while(fscanf(fp, "%d", &num) != EOF){
        sum+= num;
    }
    
    printf("Sum: %d\n", sum);

    return 0;
}
